<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="main.java.com.netcracker.course.das.server.bean.SessionVisitor" table="SESSIONS">
        <id name="id" column="SESSION_ID" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="dateInput" column="DATE_INPUT" type="java.util.Date"/>
        <property name="dateOutput" column="DATE_OUTPUT" type="java.util.Date"/>
    </class>
</hibernate-mapping>
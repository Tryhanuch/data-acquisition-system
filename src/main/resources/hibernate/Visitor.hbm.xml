<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="main.java.com.netcracker.course.das.server.bean.Visitor" table="VISITORS">
        <id name="id" column="VISITOR_ID" type="java.lang.String"> </id>
        <property name="ipAddress" column="IP_ADDRESS" type="java.lang.String"/>
        <property name="dataFirstVisit" column="DATA_FIRST_VISIT" type="java.util.Date"/>
    </class>
</hibernate-mapping>
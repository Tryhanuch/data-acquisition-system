<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="main.java.com.netcracker.course.das.server.bean.Device" table="DEVICES">
        <id name="id" column="DEVICE_ID" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property name="type" column="DEVICE_TYPE" type="java.lang.Integer"/>
        <property name="information" column="INFORMATION" type="java.lang.String"/>
        <property name="screenResolution" column="SCREEN_RESOLUTION" type="java.lang.String"/>
    </class>
</hibernate-mapping>